.page-header
    h1 Bills in the System 
        small as opposed to bills not in the system
.row
    .span7
        !=partial('messages')

        table
            thead
                tr
                    th Serial
                    th Currency
                    th Denomination
            tbody
                - bills.forEach(function(bill){
                    tr
                        td
                            a(href="/bills/#{bill.serial}")
                                = bill.serial
                        td
                            = bill.currency
                        td
                            = bill.denomination
                            
                - })
    .span3
        h3 Secondary content